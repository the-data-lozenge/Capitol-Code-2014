{
    "name": "drat-site",
    "description": "Distributed review and authoring tool",
    "keywords": [
        "express",
        "mongoose",
        "mongodb",
        "passport",
        "demo"
    ],
    "version": "0.0.0",
    "private": true,
    "author": "Dale Lotts <dlotts@knightrider.com>",
    "engines": {
        "node": ">=0.10.x",
